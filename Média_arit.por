    inteiro n1, n2, n3
    real med
	funcao inicio() {
		escreva("Digite o 1º número\n")
		leia(n1)
		escreva("Digite o 2º número\n")
		leia(n2)
		escreva("Digite o 3º número\n")
		leia(n3)
		med = (n1 + n2 + n3) / 3.0
		escreva("A média é: ", med)
	}
}
